@{
    ViewData["Title"] = "Cerrar sesion";
}

<h1>
    @ViewData["Title"]
</h1>
<form asp-action="LogoutProceso" method="post">
    <p>
        ¿Seguro que quiere cerrar sesión, @ViewBag.NombreCliente @ViewBag.ApellidoCliente?
    </p>
    <button type="submit">Aceptar</button>
    <a href="javascript:history.back()">Cancelar</a>
</form>
